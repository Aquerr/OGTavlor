<Window x:Class="OGTavlor.PictureSlideShow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OGTavlor"
        mc:Ignorable="d"
        Title="Bildspel" Height="720" Width="1280">
    <Grid>
        <Image x:Name="image" HorizontalAlignment="Left" Height="576" Margin="10,34,0,0" VerticalAlignment="Top" Width="1252" Loaded="image_Loaded" Stretch="None"/>
        <Button x:Name="Next" Content="Nästa" HorizontalAlignment="Left" Margin="978,630,0,0" VerticalAlignment="Top" Width="111" Height="49" RenderTransformOrigin="-0.308,0.992" Click="Next_Click"/>
        <Button x:Name="Previous" Content="Föregående" HorizontalAlignment="Left" Margin="219,630,0,0" VerticalAlignment="Top" Width="111" Height="49" Click="Previous_Click"/>
        <Button x:Name="BackToMainPage" Content="Startsida" HorizontalAlignment="Left" Margin="579,630,0,0" VerticalAlignment="Top" Width="111" Height="49" RenderTransformOrigin="0.933,0.547" Click="BackToMainPage_Click"/>
    </Grid>
</Window>
