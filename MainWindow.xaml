<Window x:Class="TavelProjektPT.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TavelProjektPT"
        mc:Ignorable="d"
        Title="MainWindow" Height="720" Width="1280">
    <Grid>
        <Button x:Name="LäggTillTavla" Content="Tryck för att lägga till tavlan" HorizontalAlignment="Left" Margin="124,541,0,0" VerticalAlignment="Top" Width="199" Click="button_Click" Height="47"/>
        <TextBox x:Name="SearchBox" HorizontalAlignment="Left" Height="23" Margin="982,10,0,0" TextWrapping="Wrap" Text="Sök här..." VerticalAlignment="Top" Width="120" TextChanged="SearchBox_TextChanged"/>
        <Image x:Name="image" HorizontalAlignment="Left" Height="139" Margin="84,124,0,0" VerticalAlignment="Top" Width="274" Source="logo.png" />
    <ToolBarTray HorizontalAlignment="Left" Height="26" VerticalAlignment="Top" Width="1272" Grid.ColumnSpan="2">
            <ToolBar x:Name="toolBar" HorizontalAlignment="Left" VerticalAlignment="Top" Width="60" Height="22" Header="Fil" Margin="0,4,0,-4"/>
            <ToolBar x:Name="toolBar1" HorizontalAlignment="Left" VerticalAlignment="Top" Height="21" Width="62" Header="Help" Margin="6,5,-6,-4"/>
        </ToolBarTray>
    </Grid>
</Window>
