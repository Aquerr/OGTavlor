<Window x:Class="TavelProjektPT.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TavelProjektPT"
        mc:Ignorable="d"
        Title="MainWindow" Height="720" Width="1280" ResizeMode="NoResize">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="547*"/>
            <ColumnDefinition Width="323*"/>
            <ColumnDefinition Width="402*"/>
        </Grid.ColumnDefinitions>
        <Button x:Name="LäggTillTavla" Content="Tryck för att lägga till tavlan" HorizontalAlignment="Left" Margin="124,541,0,0" VerticalAlignment="Top" Width="199" Click="button_Click" Height="47"/>
        <TextBox x:Name="SearchBox" HorizontalAlignment="Left" Height="23" Margin="112,10,0,0" TextWrapping="Wrap" Text="Sök här..." VerticalAlignment="Top" Width="120" TextChanged="SearchBox_TextChanged" Grid.Column="2" RenderTransformOrigin="0.5,0.5"/>
        <Image x:Name="image" HorizontalAlignment="Left" Height="139" Margin="77,125,0,0" VerticalAlignment="Top" Width="274" Source="logo.png" />
        <Button x:Name="TaBortTavla" Content="Tryck för att ta bort konstverk" HorizontalAlignment="Left" VerticalAlignment="Top" Width="199" Margin="124,593,0,0" Height="47" Click="button_Click_1"/>
    </Grid>
</Window>
